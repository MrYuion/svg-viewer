{
  "version": "0.0.7-development",
  "license": "MIT",
  "name": "@yuion/svg-viewer",
  "author": "Alex Sorafumo <alex@yuion.net>",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrYuion/svg-viewer.git"
  },
  "scripts": {
    "build": "node ./clean.js && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "docs": "typedoc --excludePrivate --out docs --target es6 --theme minimal --mode file src",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --runInBand",
    "lint": "tslint",
    "semantic-release": "semantic-release",
    "prepare": "npm run build",
    "demo": "npm run build && cp ./demo/australia.svg ./dist/ && parcel demo/index.html"
  },
  "peerDependencies": {
    "rxjs": "^6.6.2",
    "ts-md5": "^1.2.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "date-fns": "^2.15.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-config": "^26.2.2",
    "rxjs": "^6.6.2",
    "semantic-release": "^17.1.2",
    "ts-jest": "^26.1.4",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.7"
  }
}
